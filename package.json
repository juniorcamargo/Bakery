{
    "name": "teste-nodets-1",
    "version": "1.0.0",
    "description": "projeto teste para aprendizagem e evolu√ßao no node com TS",
    "main": "index.js",
    "author": "israel de camargo junior",
    "license": "MIT",
    "scripts": {
        "start": "tsnd -r tsconfig-paths/register --respawn --no-notify --poll --no-deps --ignore-watch node_modules --transpile-only ./src/server.ts",
        "build": "tsc",
        "lint": "yarn eslint \"./src/**/*.ts\"",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "coverage": "vitest run --coverage",
        "prepare": "husky install"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@eduzz/eslint-config-houston": "^0.52.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "@vitest/coverage-c8": "^0.30.0",
        "eslint": "^8.19.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-eslint-plugin": "^4.4.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.13.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^14.0.1",
        "prettier": "^2.7.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4",
        "vitest": "^0.30.0"
    }
}
